{
  "name": "flins",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "bun --filter '*' dev",
    "dev:web": "bun --filter 'web' dev",
    "build:web": "bun --filter 'web' build",
    "build:flins": "bun --filter 'flins' build",
    "build": "bun --filter '*' build",
    "lint": "bun --filter '*' lint",
    "fmt": "bun --filter '*' fmt",
    "lint:flins": "bun --filter 'flins' lint",
    "fmt:flins": "bun --filter 'flins' fmt",
    "lint:web": "bun --filter 'web' lint",
    "fmt:web": "bun --filter 'web' fmt"
  }
}